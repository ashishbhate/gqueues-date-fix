{
  "name": "GQueues Date Fix",
  "version": "0.1",
  "description": "Display nicer due dates in GQueues",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
   {
     "matches": ["https://www.gqueues.com/*"],
     "js": ["contentScript.js"]
   }
 ]
}
